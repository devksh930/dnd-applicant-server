# 데이터베이스 연결 정보
SPRING_DATASOURCE_URL="jdbc:mysql://localhost:3306/your_database_name"
SPRING_DATASOURCE_USERNAME="your_username"
SPRING_DATASOURCE_PASSWORD="your_password"

# 암호화 관련 정보
ENCRYPTION_TEXT_PASSWORD="your_encryption_password_hex"
ENCRYPTION_TEXT_SALT="your_encryption_salt_hex"
HMAC_KEY="your_hmac_key_base64"

# GitHub API 설정 (지원서를 GitHub 이슈로 생성하기 위함)
GITHUB_TOKEN="your_github_personal_access_token"
GITHUB_REPOSITORY_OWNER="dnd-ksh"
GITHUB_REPOSITORY_NAME="dnd-codereview"

# 서버 포트
SERVER_PORT=8088

# 사용법:
# 1. 이 파일을 .env로 복사하세요: cp .env.example .env
# 2. 실제 값으로 placeholder들을 교체하세요
# 3. .env 파일은 절대 git에 커밋하지 마세요